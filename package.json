{
  "name": "tree-sitter-glimmer",
  "version": "1.4.0",
  "description": "tree-sitter parser for glimmer syntax (and also some of handlebars)",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:ember-tooling/tree-sitter-glimmer.git.git"
  },
  "funding": "",
  "license": "MIT",
  "author": {
    "name": "NullVoxPopuli",
    "url": "github.com/nullvoxpopuli"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "glimmer"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.26.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "install": "node-gyp-build",
    "pretest": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test:node": "node --test bindings/node/*_test.js",
    "test:parser": "tree-sitter test --config-path ./test/config.json",
    "test": "npm run test:node && npm run test:parser"
  }
}
